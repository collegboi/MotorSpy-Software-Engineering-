CREATE TABLE `car` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `carReg` varchar(50) NOT NULL,
  `carMake` varchar(50) NOT NULL,
  `carModel` varchar(50) NOT NULL,
  `carDate` varchar(20) NOT NULL,
`carImage` varchar(255) NOT NULL,
  PRIMARY KEY  (`ID`)
  );
  
